<Window x:Class="BlueEyes.Views.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:BlueEyes.Views"
        xmlns:viewModels="clr-namespace:BlueEyes.ViewModels"
        mc:Ignorable="d"
        Title="LogWindow" Height="300" Width="300">

    <Window.Resources>
        <DataTemplate DataType="{x:Type viewModels:LogViewModel}">
            <views:LogView/>
        </DataTemplate>
    </Window.Resources>

    <Grid>
        <ContentControl Content="{Binding Log}"/>
    </Grid>
</Window>
